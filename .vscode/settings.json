{
  // Haxe completion server configuration
  "haxe.displayServer": {
    "print": {
      "completion": true,
      "reusing": true,
      "uncaughtError": true
    },
  },
  // Options for code generation
  "haxe.codeGeneration": {
    "functions": {
      "anonymous": {
        "argumentTypeHints": false
      }
    },
    "imports": {
      "enableAutoImports": true,
      "style": "module"
    }
  },
  // "haxe.enableMethodsView": true
   "haxe.enableDiagnostics": true,
}